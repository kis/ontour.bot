{
  "name": "telegram-bot-bootcamp",
  "version": "1.0.0",
  "description": "",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc",
    "watch": "npm run build && nodemon --watch ./src --ext ts --exec 'tsc && node .'",
    "debug": "npm run build && nodemon --legacy-watch --watch ./src --ext ts --exec node -r ts-node/register --inspect=0.0.0.0:9229 .",
    "start": "node index",
    "demon": "nodemon index",
    "dev": "pm2 start",
    "log": "pm2 monit",
    "logs": "pm2 logs ontour-bot",
    "list": "pm2 list",
    "stop": "pm2 delete ontour-bot",
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p .",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check",
    "prettier": "prettier --write ."
  },
  "author": "Kirill Stepkin",
  "license": "ISC",
  "dependencies": {
    "@types/analytics-node": "^3.1.2",
    "@types/cron": "^1.7.2",
    "@types/express": "^4.17.4",
    "@types/lodash": "^4.14.149",
    "@types/node-telegram-bot-api": "^0.40.2",
    "@types/superagent": "^4.1.7",
    "analytics-node": "^3.3.0",
    "aws-sdk": "^2.390.0",
    "bluebird": "^3.5.3",
    "body-parser": "^1.15.0",
    "cron": "^1.6.0",
    "dotenv": "^4.0.0",
    "express": "^4.13.4",
    "lodash": "^4.17.11",
    "moment": "^2.22.0",
    "node-telegram-bot-api": "^0.30.0",
    "raven": "^2.6.3",
    "superagent": "^3.8.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^10.17.18",
    "gts": "^1.1.2",
    "nodemon": "^2.0.2",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "~3.7.0"
  }
}
